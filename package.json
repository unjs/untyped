{
  "name": "untyped",
  "version": "2.0.0",
  "description": "",
  "license": "MIT",
  "repository": "unjs/untyped",
  "bin": {
    "untyped": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./babel-plugin": {
      "types": "./dist/loader/babel.d.mts",
      "default": "./dist/loader/babel.mjs"
    },
    "./loader": {
      "types": "./dist/loader/loader.d.mts",
      "default": "./dist/loader/loader.mjs"
    }
  },
  "scripts": {
    "build": "unbuild",
    "dev": "vitest",
    "lint": "oxlint . && oxfmt --check src test web",
    "lint:fix": "automd && oxlint --fix . && oxfmt src test web",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release --push && npm publish",
    "test": "pnpm lint && vitest run --coverage",
    "untyped": "jiti ./src/cli.ts",
    "web": "vite dev web",
    "web:build": "vite build web"
  },
  "dependencies": {
    "citty": "^0.2.1",
    "defu": "^6.1.4",
    "jiti": "^2.6.1",
    "knitwork": "^1.3.0",
    "scule": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/standalone": "^7.29.1",
    "@babel/template": "^7.28.6",
    "@babel/types": "^7.29.0",
    "@types/babel__standalone": "^7.1.9",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^25.3.2",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/compiler-sfc": "^3.5.29",
    "automd": "^0.4.3",
    "changelogen": "^0.6.2",
    "esbuild": "^0.27.3",
    "eslint-config-unjs": "^0.6.2",
    "hljs": "^6.2.3",
    "json-schema": "^0.4.0",
    "marked": "^17.0.3",
    "modern-monaco": "^0.3.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@tailwindcss/vite": "^4.1.7",
    "tailwindcss": "^4.1.7",
    "vite": "^6.3.5",
    "vue": "^3.5.29",
    "oxfmt": "^0.35.0",
    "oxlint": "^1.50.0",
    "prismjs": "^1.30.0",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.30.3"
}
